{
    "manifest_version": 3,
    "name": "Simplify YouTube",
    "version": "1.0.0",
    "description": "__MSG_appDesc__",
    "default_locale": "en",
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "content_scripts": [
        {
            "css": [
                "content.css"
            ],
            "run_at": "document_end",
            "matches": [
                "https://www.youtube.com/*"
            ],
            "all_frames": true
        }
    ]
}